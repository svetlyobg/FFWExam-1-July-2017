TASK3.1

<?php

#PHP OOP
#task 1 - create user class_alias

class User
{
  protected $name = " ";
  protected $email = " ";
  public function __get($param)
  {
    $this->property = $value;
    echo "$param does not exists";
  }
  public function __set($name,$value)
  {
    echo "enter your details";
    $this -> {$name} = $value;
  }
  function __call($param, $value)
  {
    echo "$param ($value)"
  }

$u1 = new User ();
$u1 -> name = "Svet";



 ?>

